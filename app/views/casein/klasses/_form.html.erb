<!-- Scaffolding generated by Casein v5.1.1.5 -->


<div class="row">
	<div class="col-lg-12 col-md-12">
		<%= casein_text_field f, f.object, :title %>
	</div>
</div>
<div class="row">
	<div class="col-lg-12 col-md-12">
		<%= casein_text_area f, f.object, :description, :class => 'description-box' %>
	</div>
</div>

<div class="row">
	<div class="col-lg-6 col-md-6">
		<%= casein_text_area f, f.object, :repertoire, :class => 'description-box' %>
	</div>
	<div class="col-lg-6 col-md-6">
		<%= casein_text_field f, f.object, :number_of_sessions  %>
	</div>
	<div class="col-lg-6 col-md-6">
		<%= casein_collection_select f, f.object, 'klass', :session_of_day, collection: Klass::SESSIONS, value_method: [1, 2, 3, 4], text_method: :session_of_day  %>
	</div>
	<div class="col-lg-6 col-md-6">
		<%= casein_text_field f, f.object, :course_id %>
	</div>
</div>