<!-- Scaffolding generated by Casein v5.1.1.5 -->

<h2>Add a new course format</h2>

<%= form_for @course_format, :url => casein_course_formats_path do |f| %>
	
	<%= render :partial => 'casein/course_formats/form', :locals => { :f => f } %>
	
	<%= f.fields_for :photos do |builder| %>
		<%= render 'casein/photos/photo_fields', f: builder %>
	<% end %>
	<br />
	
	<p class="submits">
		<%= link_to 'Cancel', casein_course_formats_path, :class => 'btn btn-mini btn-default' %>
		<%= f.submit "Add new course format", :class => "btn btn-mini btn-primary" %>
	</p>

<% end %>

<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "th-list"}Back to list".html_safe, casein_course_formats_path %></li>
<% end %>