						<% menu.children.rank(:row_order).each do |menu| %>
						<li data-update-url=<%= casein_menu_path(menu) %> class='menu-item'>
							<%= check_box_tag 'menu[menu_ids][]', menu.id, checked = false, { id: dom_id(menu), :class => 'index-check' } %>
								</td>
							<%= menu.name %>
						</li>
						<% end %>
					</ul>
				</li>
				<% end %>
			<% end %>
		</ul>
	</div>
</div>

