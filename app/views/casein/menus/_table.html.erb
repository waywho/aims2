<!-- Scaffolding generated by Casein v5.1.1.5 -->
<div class = 'col-lg-12 col-md-12 col-xs-12'>
	<div class='col-lg-6 col-md-6 col-xs-12'>
		<ul class="main-menus menu-list">
			<h4>Main Menu</h4>
			<% menus.main.roots.each do |menu| %>
				<li data-update-url=<%= casein_menu_path(menu) %> class='menu-item'>
					<%= check_box_tag 'menu[menu_ids][]', menu.id, checked = false, { id: dom_id(menu), :class => 'index-check' } %>
					<%= menu.name %>
					<% if menu.children? %>
					<ul class='menu-list nested-menu-list'>
						<% menu.children.rank(:row_order).each do |menu| %>
						<li data-update-url=<%= casein_menu_path(menu) %> class='menu-item'>
							<%= check_box_tag 'menu[menu_ids][]', menu.id, checked = false, { id: dom_id(menu), :class => 'index-check' } %>
								</td>
							<%= menu.name %>
						</li>
						<% end %>
					</ul>
				</li>
				<% end %>
			<% end %>
		</ul>
	</div>

	<div class='col-lg-6 col-md-6 col-xs-12'>
		<ul class="secondary-menus menu-list">
			<h4>Secondary Menu</h4>
			<% menus.secondary.roots.each do |menu| %>
