	<br />
	Upload CSV file:
	<br />
	<%= form_tag import_casein_courseformats_path, multipart: true do %>
		<%= file_field_tag :file, accept: 'text/csv' %>
		<%= submit_tag "Import", :class => 'btn btn-xs btn-primary pull-right' %>
	<% end %>

<% end %>
