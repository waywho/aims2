<!-- Scaffolding generated by Casein v5.1.1.5 -->

<h2>Add a new quote</h2>

<%= form_for @quote, :url => casein_quotes_path do |f| %>
	
	<%= render :partial => 'casein/quotes/form', :locals => { :f => f } %>
	
	<p class="submits">
		<%= link_to 'Cancel', casein_quotes_path, :class => 'btn btn-mini btn-danger' %>
		<%= f.submit "Add new quote", :class => "btn btn-mini btn-primary" %>
	</p>

<% end %>

<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "th-list"}Back to list".html_safe, casein_quotes_path %></li>
<% end %>