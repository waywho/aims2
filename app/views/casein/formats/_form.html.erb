<!-- Scaffolding generated by Casein v5.1.1.5 -->


<div class="row">
	<div class="col-lg-12 col-md-12">
		<%= casein_text_field f, f.object, :title %>
	</div>
</div>
<div class="row">
	<div class="col-lg-9 col-md-9">
		<%= casein_text_area f, f.object, :description, :class => "description-box" %>
	</div>
	<div class="col-lg-3 col-md-3">
		<%= casein_datetime_field f, f.object, :when_from %>
		<%= casein_datetime_field f, f.object, :when_to %>
	</div>
</div>
<div class="row">
	<div class="col-lg-6 col-md-6">
		<%= casein_text_area f, f.object, :whats_new, :class => "description-box" %>
	</div>
	<div class="col-lg-6 col-md-6">
		<%= casein_text_field f, f.object, :venue %>

		<%= casein_text_field f, f.object, :address1 %>

		<%= casein_text_field f, f.object, :address2 %>
	
		<div class="row">
			<div class="col-lg-6 col-md-6">
				<%= casein_text_field f, f.object, :city %>
			</div>
			<div class="col-lg-6 col-md-6">
				<%= casein_text_field f, f.object, :county %>
			</div>
		</div>
		<div class="row">
			<div class="col-lg-6 col-md-6">
				<%= casein_text_field f, f.object, :country %>
			</div>
			<div class="col-lg-6 col-md-6">
				<%= casein_text_field f, f.object, :post_code %>
			</div>
		</div>
	</div>
</div>
