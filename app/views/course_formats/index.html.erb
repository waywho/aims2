<p id="notice"><%= notice %></p>

<h1>Listing Course Formats</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_formats.each do |course_format| %>
      <tr>
        <td><%= link_to 'Show', course_format %></td>
        <td><%= link_to 'Edit', edit_course_format_path(course_format) %></td>
        <td><%= link_to 'Destroy', course_format, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course format', new_course_format_path %>
